package code.binaryTree;

public class Node {

	public int value;
	public Node leftNode;
	public Node rightNode;
	
	public static void BFT(Node n) {
		if(n== null)
			return;
	
	 BFT(n.leftNode);
	 BFT(n.rightNode);
	 System.out.println("node val " + n.value);
	}
	
	Node(int val){
		this.value = val;
	}
	
	private static void constructNode() {
		Node n = new Node(8);
		Node n1 = new Node(6);
		Node n2 = new Node(7);
		Node n3 = new Node(4);
		Node n4 = new Node(5);
		Node n5 = new Node(3);
		Node n6 = new Node(2);
		Node n7 = new Node(1);
		Node n8 = new Node(0);
		
	}
}
